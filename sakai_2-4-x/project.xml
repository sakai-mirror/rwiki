<?xml version="1.0" encoding="UTF-8"?>
<project>
    <pomVersion>3</pomVersion>
    <name>sakai-rwiki-multiproject</name>
    <extend>../master/project.xml</extend>

    <groupId>sakaiproject</groupId>
    <artifactId>sakai-rwiki-multiproject</artifactId>
    <currentVersion>${sakai.version}</currentVersion>
    <shortDescription>Sakai RWiki is an embedded Wiki Tool for Sakai</shortDescription>
    <description>This is the base project for the Sakai RWiki project. The project currently
    uses three child projects in a multiproject fashion.</description>
    <inceptionYear>2005</inceptionYear>
    <dependencies>
        <!-- our plugin -->
        <dependency>
            <groupId>sakaiproject</groupId>
            <artifactId>sakai</artifactId>
            <version>${sakai.plugin.version}</version>
            <type>plugin</type>
        </dependency>
    </dependencies>
</project>

